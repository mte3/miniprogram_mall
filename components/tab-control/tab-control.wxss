/* components/tab-control/tab-control.wxss */
.tab{
    display: flex;
    height: 88rpx;
    background-color: white;
}
.tabItem{
    flex: 1;
    text-align: center;
    line-height: 88rpx;
    font-size: 32rpx;
}
.active{
    color: #ff5777;
}
.active>.a{
    padding: 12rpx;
    border-bottom: 6rpx solid #ff5777;
}